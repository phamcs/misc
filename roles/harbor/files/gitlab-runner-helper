#!/bin/bash
gitlabrh="docker.io/gitlab/gitlab-runner-helper"
superasian="harbor.superasian.net/library/gitlab/gitlab-runner-helper"
tags=("x86_64-v17.11.2"
      "ubuntu-x86_64-v17.11.2"
      "alpine3.21-x86_64-v17.11.2"
      "x86_64-v18.0.2"
      "ubuntu-x86_64-v18.0.2"
      "alpine3.21-x86_64-v18.0.2")
    for tag in ${tags[@]}; do
        docker pull $gitlabrh:$tag
        docker tag $gitlabrh:$tag $superasian:$tag
        docker push $superasian:$tag
        docker rmi $gitlabrh:$tag
    done
exit 0;
